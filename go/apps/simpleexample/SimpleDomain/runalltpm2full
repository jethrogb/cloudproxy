#
source ./defines
sudo $BINPATH/tao host init -tao_domain $DOMAINTPM2 -stacked -parent_type TPM2 -hosting process
sleep 1
sudo $BINPATH/tao host start -tao_domain $DOMAINTPM2 &
sleep 1
$BINPATH/domain_server -pass xxx
sleep 1
$BINPATH/tao run -tao_domain $DOMAINTPM2 -domain_config "$DOMAINTPM2/tao.config" \
-path "/$DOMAINTPM2/SimpleServer" $BINPATH/SimpleServer \
-use_simpledomainservice=false &
sleep 1
$BINPATH/tao run -tao_domain $DOMAINTPM2 -domain_config "$DOMAINTPM2/tao.config" \
-path "/$DOMAINTPM2/SimpleServer" $BINPATH/SimpleClient \
-use_simpledomainservice=false &

