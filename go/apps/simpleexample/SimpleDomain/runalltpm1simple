#
source ./defines
sudo $BINPATH/tao host init -tao_domain . -stacked -parent_type TPM1 -hosting process
sleep 1
sudo $BINPATH/tao host start -tao_domain . &
sleep 1
sudo $BINPATH/tao run -tao_domain . $BINPATH/SimpleDomainService \
-config tao.config -service_path $DOMAINTPM1/SimpleDomainService &
sleep 1
sudo $BINPATH/tao run -tao_domain . -domain_config "$DOMAINTPM1/tao.config" \
-path "/$DOMAINTPM1/SimpleServer" -service_path $DOMAINTPM1/SimpleServer &
sleep 1
sudo $BINPATH/tao run -tao_domain . -domain_config "$DOMAINTPM1/tao.config" \
-path "/$DOMAINTPM1/SimpleClient" -service_path $DOMAINTPM1/SimpleClient &

