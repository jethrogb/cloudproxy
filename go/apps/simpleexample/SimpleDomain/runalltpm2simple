#
source ./defines
sudo $BINPATH/tao host init -tao_domain . -stacked -parent_type TPM2 -hosting process
sleep 1
sudo $BINPATH/tao host start -tao_domain . &
sleep 1
sudo $BINPATH/tao run -tao_domain . $BINPATH/SimpleDomainService \
-config tao.config -service_path $DOMAINTPM2/SimpleDomainService &
sleep 1
sudo $BINPATH/tao run -tao_domain . -domain_config "$DOMAINTPM2/tao.config" \
-path "/$DOMAINTPM2/SimpleServer" -service_path $DOMAINTPM2/SimpleServer &
sleep 1
sudo $BINPATH/tao run -tao_domain .  -domain_config "$DOMAINTPM2/tao.config" \
-path "/$DOMAINTPM2/SimpleClient" -service_path $DOMAINTPM2/SimpleClient &

